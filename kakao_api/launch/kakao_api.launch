<?xml version="1.0"?>
<launch>
  <!-- Kakao API GPS 서버 및 웨이포인트 관리 -->

  <!-- 시뮬레이션 시간 설정 -->
  <param name="use_sim_time" value="true"/>

  <!-- 웹 인터페이스 및 GPS 서버 -->
  <node pkg="kakao_api" type="gps_server.py" name="gps_server_node" output="screen">
    <param name="port" value="8000"/>
    <param name="websocket_port" value="8765"/>
    <param name="waypoints_websocket_port" value="8766"/>
  </node>

  <!-- 웨이포인트 자율주행 네비게이션 -->
  <node pkg="kakao_api" type="waypoints_manager.py" name="waypoint_manager_node" output="screen"/>

</launch>
