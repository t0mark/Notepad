<?xml version="1.0"?>
<launch>
  <!-- Kakao API Debug Launch File -->
  <!-- 초기 위치: 35.846171, 127.134468 -->

  <!-- 시뮬레이션 시간 설정 -->
  <param name="use_sim_time" value="false"/>

  <!-- 디버그용 GPS 발행 노드 (1Hz 지속 발행) -->
  <node pkg="kakao_api" type="debug_gps_publisher.py" name="debug_gps_publisher" output="screen"/>

  <!-- 웹 인터페이스 서버 (HTTP + WebSocket) -->
  <node pkg="kakao_api" type="web_server.py" name="web_server" output="screen"/>

  <!-- 카카오 경로 계획 노드 (GPS → map 프레임 변환) -->
  <node pkg="kakao_api" type="kakao_route_planner.py" name="kakao_route_planner" output="screen"/>

</launch>
